{"hansard": {"@id": "", "@tocId": "", "@xml:lang": "EN-AU", "@schemaVersion": "1.0", "@xsi:noNamespaceSchemaLocation": "hansard_1_0.xsd", "@xmlns:xlink": "http://www.w3.org/1999/xlink", "@xmlns:xsi": "http://www.w3.org/2007/XMLSchema-instance", "@xmlns:mml": "http://www.w3.org/1998/Math/MathML", "name": "House of Assembly", "date": {"@date": "2019-02-12"}, "sessionName": "Fifty-Fourth Parliament, First Session (54-1)", "parliamentNum": "54", "sessionNum": "1", "parliamentName": "Parliament of South Australia", "house": "House of Assembly", "venue": null, "reviewStage": "published", "startPage": {"@num": "4425"}, "endPage": {"@num": "4542"}, "dateModified": {"@time": "2019-05-13T17:16:01+09:30"}, "proceeding": {"@continued": "true", "name": "Answers to Questions", "subject": {"name": "Police Numbers", "text": {"@id": "20190212851134bb8ecc4b5ca0000001", "inserted": {"heading": "Police Numbers"}}, "talker": [{"@role": "member", "@id": "4335", "@kind": "question", "name": "Mr ODENWALDER", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Elizabeth"}, "questions": {"question": {"@date": "2018-12-05", "@qonNum": "517", "name": "Police Numbers"}}, "text": [{"@id": "20190212851134bb8ecc4b5ca0000002", "inserted": {"by": {"@role": "member", "@id": "4335", "#text": "Mr ODENWALDER (Elizabeth)"}, "#text": "517  (5 December 2018).  What was the total number of sworn police officers (FTE), at 31 July 2018, of the rank of:"}}, {"@id": "20190212851134bb8ecc4b5ca0000003", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Constable (including probationary constable)?"}}, {"@id": "20190212851134bb8ecc4b5ca0000004", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Senior Constable?"}}, {"@id": "20190212851134bb8ecc4b5ca0000005", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Sergeant?"}}, {"@id": "20190212851134bb8ecc4b5ca0000006", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Senior Sergeant?"}}, {"@id": "20190212851134bb8ecc4b5ca0000007", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Inspector?"}}, {"@id": "20190212851134bb8ecc4b5ca0000008", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Chief Inspector?"}}]}, {"@role": "member", "@id": "4843", "@kind": "answer", "name": "The Hon. C.L. WINGARD", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Gibson"}, "portfolios": {"portfolio": [{"@id": "", "name": "Minister for Police"}, {"@id": "", "name": "Minister for Recreation"}]}, "questions": {"question": {"@date": "2018-12-05", "@qonNum": "517", "name": "Police Numbers"}}, "text": [{"@id": "20190212851134bb8ecc4b5ca0000009", "inserted": {"by": {"@role": "member", "@id": "4843", "#text": "The Hon. C.L. WINGARD (Gibson\u00e2\u20ac\u201dMinister for Police, Emergency Services and Correctional Services, Minister for Recreation, Sport and Racing):"}}}, {"@id": "20190212851134bb8ecc4b5ca0000010", "inserted": "Total number of sworn police officers (FTE) as at 31 July 2018, of the rank of:"}, {"@id": "20190212851134bb8ecc4b5ca0000011", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Constable \u00e2\u20ac\u201c 966.2 FTE. This includes 363 Probationary Constables and 603.2 Constables."}}, {"@id": "20190212851134bb8ecc4b5ca0000012", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Senior Constable \u00e2\u20ac\u201c 1,608.6 FTE. This includes 1058.3 Senior Constables and 550.3 Senior Constables First Class."}}, {"@id": "20190212851134bb8ecc4b5ca0000013", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Sergeant \u00e2\u20ac\u201c 629.0 FTE"}}, {"@id": "20190212851134bb8ecc4b5ca0000014", "item": {"@sublevel": "1", "@bullet": "true", "inserted": {"term": null, "#text": "Senior Sergeant \u00e2\u20ac\u201c 220.8 FTE. This includes 88.0 Senior Sergeants and 132.8 Senior Sergeants First Class"}}}, {"@id": "20190212851134bb8ecc4b5ca0000015", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Inspector \u00e2\u20ac\u201c 58.8 FTE"}}, {"@id": "20190212851134bb8ecc4b5ca0000016", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Chief Inspector \u00e2\u20ac\u201c 42.0 FTE."}}]}]}}}}