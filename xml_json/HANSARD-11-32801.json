{"hansard": {"@id": "", "@tocId": "", "@xml:lang": "EN-AU", "@schemaVersion": "1.0", "@xsi:noNamespaceSchemaLocation": "hansard_1_0.xsd", "@xmlns:xlink": "http://www.w3.org/1999/xlink", "@xmlns:xsi": "http://www.w3.org/2007/XMLSchema-instance", "@xmlns:mml": "http://www.w3.org/1998/Math/MathML", "name": "House of Assembly", "date": {"@date": "2019-02-12"}, "sessionName": "Fifty-Fourth Parliament, First Session (54-1)", "parliamentNum": "54", "sessionNum": "1", "parliamentName": "Parliament of South Australia", "house": "House of Assembly", "venue": null, "reviewStage": "published", "startPage": {"@num": "4425"}, "endPage": {"@num": "4542"}, "dateModified": {"@time": "2019-05-13T17:16:01+09:30"}, "proceeding": {"@continued": "true", "name": "Answers to Questions", "subject": {"name": "Auditor-General's Report", "text": {"@id": "201902125c95fcdd5980460980000001", "inserted": {"heading": "Auditor-General's Report"}}, "talker": [{"@role": "member", "@id": "5376", "@kind": "question", "name": "In reply to Ms STINSON", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Badcoe"}, "questions": {"question": {"@date": "2019-02-12", "name": "Auditor-General's Report"}}, "text": {"@id": "201902125c95fcdd5980460980000002", "inserted": {"by": {"@role": "member", "@id": "5376", "#text": "Ms STINSON (Badcoe)"}, "#text": "In reply to  (28 November 2018)."}}}, {"@role": "member", "@id": "4340", "@kind": "answer", "name": "The Hon. R. SANDERSON", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Adelaide"}, "portfolios": {"portfolio": {"@id": "", "name": "Minister for Child Protection"}}, "questions": {"question": {"@date": "2019-02-12", "name": "Auditor-General's Report"}}, "text": [{"@id": "201902125c95fcdd5980460980000003", "inserted": {"by": {"@role": "member", "@id": "4340", "#text": "The Hon. R. SANDERSON (Adelaide\u00e2\u20ac\u201dMinister for Child Protection):"}, "#text": "I have been advised:"}}, {"@id": "201902125c95fcdd5980460980000004", "inserted": "As at the 30 June 2018, the total number of children in out-of-home-care is 3,695:"}, {"@id": "201902125c95fcdd5980460980000005", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Commercial care \u00e2\u20ac\u201c 93"}}, {"@id": "201902125c95fcdd5980460980000006", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Foster Care \u00e2\u20ac\u201c 1,434"}}, {"@id": "201902125c95fcdd5980460980000007", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Kinship Care (inclusive of Specified Child only carer) \u00e2\u20ac\u201c 1,684"}}, {"@id": "201902125c95fcdd5980460980000008", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Residential Care \u00e2\u20ac\u201c 408"}}, {"@id": "201902125c95fcdd5980460980000009", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Independent Living \u00e2\u20ac\u201c 47"}}, {"@id": "201902125c95fcdd5980460980000010", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Family day care\u00e2\u20ac\u201d29"}}, {"@continued": "true", "@id": "201902125c95fcdd5980460980000011", "inserted": "Please note:"}, {"@id": "201902125c95fcdd5980460980000012", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Data is correct as at 30 June 2018."}}, {"@id": "201902125c95fcdd5980460980000013", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Previous data adjustments occur due to information updates within the client management system. Data has now been frozen as at 31 August 2018."}}, {"@id": "201902125c95fcdd5980460980000014", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Data is counted for children aged 0-17, consistent with National Australian institute of Health and Welfare (AIHW) and Report on Government Services (ROGS) reporting."}}, {"@id": "201902125c95fcdd5980460980000015"}]}]}}}}