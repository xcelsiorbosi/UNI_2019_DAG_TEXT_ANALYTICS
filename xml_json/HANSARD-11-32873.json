{"hansard": {"@id": "", "@tocId": "", "@xml:lang": "EN-AU", "@schemaVersion": "1.0", "@xsi:noNamespaceSchemaLocation": "hansard_1_0.xsd", "@xmlns:xlink": "http://www.w3.org/1999/xlink", "@xmlns:xsi": "http://www.w3.org/2007/XMLSchema-instance", "@xmlns:mml": "http://www.w3.org/1998/Math/MathML", "name": "House of Assembly", "date": {"@date": "2019-02-13"}, "sessionName": "Fifty-Fourth Parliament, First Session (54-1)", "parliamentNum": "54", "sessionNum": "1", "parliamentName": "Parliament of South Australia", "house": "House of Assembly", "venue": null, "reviewStage": "published", "startPage": {"@num": "4543"}, "endPage": {"@num": "4648"}, "dateModified": {"@time": "2019-06-05T16:45:27+09:30"}, "proceeding": {"name": "Answers to Questions", "text": {"@id": "20190213cb7ed6edf6e44b0d80000001", "heading": "Answers to Questions"}, "subject": {"name": "Hospital Presentations", "text": {"@id": "20190213cb7ed6edf6e44b0d80000002", "inserted": {"heading": "Hospital Presentations"}}, "talker": [{"@role": "member", "@id": "4841", "@kind": "question", "name": "Mr PICTON", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Kaurna"}, "questions": {"question": {"@date": "2018-02-13", "@qonNum": "542", "name": "Hospital Presentations"}}, "text": {"@id": "20190213cb7ed6edf6e44b0d80000003", "inserted": {"by": {"@role": "member", "@id": "4841", "#text": "Mr PICTON (Kaurna)"}, "#text": "542  (13 February 2018).  What is the total number of hospital presentations, statewide and for each metro hospital and country in total, for 2016-17 and 2017-18?"}}}, {"@role": "member", "@id": "4342", "@kind": "answer", "name": "The Hon. D.C. VAN HOLST PELLEKAAN", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Stuart"}, "portfolios": {"portfolio": {"@id": "", "name": "Minister for Energy and Mining"}}, "questions": {"question": {"@date": "2018-02-13", "@qonNum": "542", "name": "Hospital Presentations"}}, "text": [{"@id": "20190213cb7ed6edf6e44b0d80000004", "inserted": {"by": {"@role": "member", "@id": "4342", "#text": "The Hon. D.C. VAN HOLST PELLEKAAN (Stuart\u00e2\u20ac\u201dMinister for Energy and Mining):"}, "#text": "The Minister for Health and Wellbeing has been advised:"}}, {"@id": "20190213cb7ed6edf6e44b0d80000005", "inserted": "In 2017-18, hospital presentations were 605,730 across the state."}, {"@id": "20190213cb7ed6edf6e44b0d80000006", "inserted": "In 2016-17, hospital presentations were 587,466 across the state."}, {"@id": "20190213cb7ed6edf6e44b0d80000007", "inserted": {"table": {"rowtitle": {"cell": ["Hospital", "2016-17", "2017-18"]}, "row": [{"cell": ["Flinders Medical Centre", "85,778", "88,890"]}, {"cell": ["Lyell McEwin Hospital", "71,850", "73,475"]}, {"cell": ["Modbury Hospital", "36,864", "40,804"]}, {"cell": ["Noarlunga Hospital", "40,470", "40,886"]}, {"cell": ["Royal Adelaide Hospital", "77,625", "77,693"]}, {"cell": ["The Queen Elizabeth Hospital", "44,725", "45,256"]}, {"cell": ["Women's and Children's Hospital", "46,581", "45,931"]}, {"cell": ["Country", "183,573", "192,795"]}, {"cell": ["Total of all presentations", "587,466", "605,730"]}]}}}, {"@id": "20190213cb7ed6edf6e44b0d80000008"}]}]}}}}