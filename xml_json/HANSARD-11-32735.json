{"hansard": {"@id": "", "@tocId": "", "@xml:lang": "EN-AU", "@schemaVersion": "1.0", "@xsi:noNamespaceSchemaLocation": "hansard_1_0.xsd", "@xmlns:xlink": "http://www.w3.org/1999/xlink", "@xmlns:xsi": "http://www.w3.org/2007/XMLSchema-instance", "@xmlns:mml": "http://www.w3.org/1998/Math/MathML", "name": "House of Assembly", "date": {"@date": "2019-02-12"}, "sessionName": "Fifty-Fourth Parliament, First Session (54-1)", "parliamentNum": "54", "sessionNum": "1", "parliamentName": "Parliament of South Australia", "house": "House of Assembly", "venue": null, "reviewStage": "published", "startPage": {"@num": "4425"}, "endPage": {"@num": "4542"}, "dateModified": {"@time": "2019-05-13T17:16:01+09:30"}, "proceeding": {"@continued": "true", "name": "Answers to Questions", "subject": {"name": "Police Numbers", "text": {"@id": "2019021250365893cff3430490000001", "inserted": {"heading": "Police Numbers"}}, "talker": [{"@role": "member", "@id": "4335", "@kind": "question", "name": "Mr ODENWALDER", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Elizabeth"}, "questions": {"question": {"@date": "2018-12-05", "@qonNum": "518", "name": "Police Numbers"}}, "text": [{"@id": "2019021250365893cff3430490000002", "inserted": {"by": {"@role": "member", "@id": "4335", "#text": "Mr ODENWALDER (Elizabeth)"}, "#text": "518  (5 December 2018).  What was the total number of sworn police officers (FTE), at 31 October 2018, of the rank of:"}}, {"@id": "2019021250365893cff3430490000003", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Constable (including probationary constable)?"}}, {"@id": "2019021250365893cff3430490000004", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Senior Constable?"}}, {"@id": "2019021250365893cff3430490000005", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Sergeant?"}}, {"@id": "2019021250365893cff3430490000006", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Senior Sergeant?"}}, {"@id": "2019021250365893cff3430490000007", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Chief Inspector?"}}], "page": {"@num": "4522"}}, {"@role": "member", "@id": "4843", "@kind": "answer", "name": "The Hon. C.L. WINGARD", "house": "House of Assembly", "electorate": {"@id": "", "#text": "Gibson"}, "portfolios": {"portfolio": [{"@id": "", "name": "Minister for Police"}, {"@id": "", "name": "Minister for Recreation"}]}, "questions": {"question": {"@date": "2018-12-05", "@qonNum": "518", "name": "Police Numbers"}}, "text": [{"@id": "2019021250365893cff3430490000008", "inserted": {"by": {"@role": "member", "@id": "4843", "#text": "The Hon. C.L. WINGARD (Gibson\u00e2\u20ac\u201dMinister for Police, Emergency Services and Correctional Services, Minister for Recreation, Sport and Racing):"}}}, {"@id": "2019021250365893cff3430490000009", "inserted": "Total number of sworn police officers (FTE) as at 31 October 2018, of the rank of:"}, {"@id": "2019021250365893cff3430490000010", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Constable \u00e2\u20ac\u201c 923.4 FTE. This includes 329 Probationary Constables and 594.4 Constables."}}, {"@id": "2019021250365893cff3430490000011", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Senior Constable \u00e2\u20ac\u201c 1,597.2 FTE. This includes 1062.9 Senior Constables and 534.3 Senior Constables First Class."}}, {"@id": "2019021250365893cff3430490000012", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Sergeant \u00e2\u20ac\u201c 631.3 FTE"}}, {"@id": "2019021250365893cff3430490000013", "item": {"@sublevel": "1", "@bullet": "true", "inserted": {"term": null, "#text": "Senior Sergeant \u00e2\u20ac\u201c 207.4 FTE. This includes 82.0 Senior Sergeants and 125.4 Senior Sergeants First Class"}}}, {"@id": "2019021250365893cff3430490000014", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Inspector \u00e2\u20ac\u201c 61.0 FTE"}}, {"@id": "2019021250365893cff3430490000015", "item": {"@sublevel": "1", "@bullet": "true", "inserted": "Chief Inspector \u00e2\u20ac\u201c 43.0 FTE."}}]}]}}}}